class Solution(object):
    def romanToInt(self, s):
        rom_to_int={'I':1,'V':5,'X':10,'L':50,'C':100,'D':500,'M':1000}
        result = 0
        for i in range(len(s)):
            cval = rom_to_int[s[i]]
            nval = rom_to_int[s[i+1]] if i+1 < len(s) else :           
        if  cval < nval:
                total -= cval
            else:
                  total += cval
        return total


